  <table-filter match-name="usuarios"/>
  <table-filter match-name="estados_reserva"/>
  <table-filter match-name="provincias"/>
  <table-filter match-name="reservas_cab"/>
  <table-filter match-name="reservas_det"/>
  <table-filter match-name="consumos"/>
  <table-filter match-name="tipos_habitacion"/>
